{
  "trailingComma": "none",
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "tailwindConfig": "./styles/tailwind.config.js",
  "tailwindPreserveWhitespace": true,
  "tailwindPreserveDuplicates": true,
  "plugins": [
    "prettier-plugin-svelte",
    "prettier-plugin-organize-imports",
    "prettier-plugin-tailwindcss"
  ]
}